private void btnOutput08_Click(object sender, EventArgs e)
 {
     var data1 = int.Parse(tbxInput1.Text);
     var data2 = int.Parse(tbxInput2.Text);

     string result;  // 지역변수 - 초기화가 자동으로 안됨
     if (data1 == data2) {
         result = "같아요";
     } else if (data1 < data2) {
         result = "뒤가 커요";
     } else {  // if (data1 > data2) {
         result = "앞이 커요";
     }

         lblResult.Text = result;
 }
